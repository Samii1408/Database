Descending order
CREATE TABLE SAMRA 
(
  id INT,
  Name Varchar (20),
  Age INT,
  Grade Varchar (5)
);
INSERT INTO SAMRA (ID,NAME,AGE,GRADE)
VALUES ( 01,'SAMRA',20,'A');
INSERT INTO SAMRA (ID,NAME,AGE,GRADE)
VALUES (02,'ZOYA',19,'B');
INSERT INTO SAMRA( ID,NAME,AGE,GRADE)
VALUES (03,'MANO',18,'C');
INSERT INTO SAMRA (ID,NAME,AGE,GRADE)
VALUES(04,'MALIKA',20,'D');
SELECT * FROM SAMRA 
ORDER BY ID DESC;
